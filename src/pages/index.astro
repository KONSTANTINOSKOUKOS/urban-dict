---
import NavLayout from "../layouts/NavLayout.astro";
import Post from "../components/Post";
import { getAllPosts, getPostsByLetter } from "../trpc/client";

const posts = await getAllPosts();
const fromN = await getPostsByLetter("N");
---

<NavLayout title="Home Page for Urban Dict">
	{fromN.map((post) => <Post post={post} />)}
	<hr class="h-10"/>
	{posts.map((post) => <Post post={post} />)}
</NavLayout>
